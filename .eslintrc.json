{
    "env": {
      "es6": true
    },
    "extends": ["eslint:recommended"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "parser": "babel-eslint",
    "rules": {
      "no-throw-literal": 0,
      "no-undef": 0,
      "react/prop-types": 0,
      "react/display-name": 0
    }
  }